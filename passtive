import java.io.*;
import java.util.*;
class passtive
{
public static void main(String args[])
{
int n;
Scanner k=new Scanner(System.in);
n=k.nextInt();
if(n>0)
{
	System.out.println("the no is posstive");
}
else if(n<0)
{
		System.out.println("the no is negative");
}
else
{
		System.out.println("the no is zero");
}
}
}
